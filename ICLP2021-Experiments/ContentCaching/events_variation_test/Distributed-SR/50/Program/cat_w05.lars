high(C) :- value(V), alpha(C,V) in [5 sec], catalog0(C), 12 <= V.
mid(C) :- value(V), alpha(C,V) in [5 sec], catalog0(C),  12 <= V, V < 18.
low(C) :- value(V), alpha(C,V) in [5 sec], catalog0(C), V <= 12.
lfu(C) :- high(C) always in [5 sec], catalog1(C).
lru(C) :- mid(C) always in [5 sec], catalog1(C).
fifo(C) :- low(C) always in [5 sec], rtm50(C) in [5 sec], catalog1(C).
done(C) :- lfu(C).
done(C) :- lru(C).
done(C) :- fifo(C).
random(C) :- not done(C),catalog1(C).
finish(C) :- off(C) in [1 sec], done(C).
finish(C) :- off(C) in [1 sec], random(C).
